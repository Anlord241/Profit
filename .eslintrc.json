{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "spaced-comment": "off",
        "linebreak-style": "off",
        "no-console": "off",
       
        "no-underscore-dangle": "off",
        "prefer-const": "off"

    }
}
